{
  "name": "koa-framework",
  "version": "0.2.0",
  "description": "Koa framework",
  "main": "dist",
  "scripts": {
    "build": "tsc"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "bammoo/koa-framework",
  "author": "John Xiao <xiao-jianfeng@qq.com>",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "fs-extra": "4.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-jwt": "^3.5.1",
    "koa-morgan": "^1.0.1",
    "koa-pino-logger": "^2.1.3",
    "koa-router": "^7.4.0",
    "pino": "^5.11.3",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "shortid": "^2.2.11"
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/koa": "^2.0.48",
    "@types/koa-router": "^7.0.40",
    "@types/node": "^9.4.6",
    "eslint": "^6.0.1",
    "pino-pretty": "^2.5.0",
    "typescript": "^3.3.3"
  }
}
